apiVersion: cert-manager.io/v1
kind: Certificate
metadata:
  name: gitlab-cert
  namespace: "{{ gitlab_namespace }}"
spec:
  commonName: gitlab
  dnsNames:
    - "*.apps.cluster-qq995.sandbox2890.opentlc.com"
  usages:
    - server auth
  issuerRef:
    kind: ClusterIssuer
    name: letsencrypt-staging
  secretName: sct-gitlab-tls
